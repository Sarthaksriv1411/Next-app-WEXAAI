{
  "name": "welcome-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build -t nextjs-welcome-app .",
    "docker:run": "docker run -p 3000:3000 nextjs-welcome-app",
    "docker:local": "chmod +x scripts/run-local.sh && ./scripts/run-local.sh",
    "k8s:setup": "chmod +x scripts/setup-minikube.sh && ./scripts/setup-minikube.sh",
    "k8s:deploy": "chmod +x scripts/deploy-minikube.sh && ./scripts/deploy-minikube.sh",
    "k8s:cleanup": "chmod +x scripts/cleanup.sh && ./scripts/cleanup.sh"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
